{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid"
    },
    "heroImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "richTextLite": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "light"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "richTextStandard": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "standard"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "richTextRich": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    }
  }
}
